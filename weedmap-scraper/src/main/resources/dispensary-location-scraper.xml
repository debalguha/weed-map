<?xml version="1.0" encoding="UTF-8"?>

<config>
	<var-def name="webpage">
		<html-to-xml>
			<http url="#URL#"></http>
		</html-to-xml>
	</var-def>	
	<var-def name="urls">
		<xpath expression="//div[@class='listings']//div[@class='body']//h3/a/@href">
			<var name="webpage"/>
		</xpath>
	</var-def>
</config>